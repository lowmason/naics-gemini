# Personal zsh customizations
# Copy this to ~/.zshrc.local and customize
# cp scripts/zshrc.local.example ~/.zshrc.local

# Aliases
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'

# Project-specific aliases
alias naics="uv run naics-embedder"
alias naics-train="uv run naics-embedder train"
alias naics-data="uv run naics-embedder data"
alias naics-tools="uv run naics-embedder tools"

# Functions
naics-setup() {
    cd ~/naics-embedder
    ./scripts/setup.sh
}

